{
  "AppSettings": {
    "ApplicationName": "VPN Location Changer",
    "Version": "1.0.0",
    "DefaultProxyPort": 1080,
    "DefaultServerPort": 8888,
    "ConnectionTimeout": 30,
    "AutoCheckIPOnConnect": true,
    "LogLevel": "Info"
  },
  "Paths": {
    "ServersConfig": "servers.json",
    "PythonScript": "socks5_proxy.py",
    "LogFile": "vpn_log.txt"
  },
  "UI": {
    "Theme": "Light",
    "WindowWidth": 500,
    "WindowHeight": 600,
    "ShowNotifications": true
  },
  "API": {
    "IPCheckService": "https://api.ipify.org?format=json",
    "LocationService": "https://ipapi.co/{ip}/json/",
    "Timeout": 10
  }
}

